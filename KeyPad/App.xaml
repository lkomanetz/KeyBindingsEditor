<Application x:Class="KeyPad.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:KeyPad"
			 xmlns:kbEditorVm="clr-namespace:KeyPad.KeyBindingsEditor.ViewModels"
			 xmlns:kbEditor="clr-namespace:KeyPad.KeyBindingsEditor"
			 xmlns:pwVm="clr-namespace:KeyPad.ProcessWatcher.ViewModels"
			 xmlns:pw="clr-namespace:KeyPad.ProcessWatcher"
			 xmlns:settings="clr-namespace:KeyPad.Settings"
			 xmlns:settingsVm="clr-namespace:KeyPad.Settings.ViewModels"
			 StartupUri="MainWindow.xaml">
	<Application.Resources>
		<DataTemplate DataType="{x:Type kbEditorVm:KeyBindingsEditorViewModel}">
			<kbEditor:KeyBindingsEditor/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type pwVm:ProcessWatcherViewModel}">
			<pw:ProcessWatcher/>
		</DataTemplate>
		<DataTemplate DataType="{x:Type settingsVm:KeyPadSettingsViewModel}">
			<settings:KeyPadSettings/>
		</DataTemplate>
	</Application.Resources>
</Application>
